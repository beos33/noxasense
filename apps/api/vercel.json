{
  "version": 2,
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        },
        {
          "key": "Access-Control-Max-Age",
          "value": "7200"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=7200"
        },
        {
          "key": "Vary",
          "value": "Origin"
        }
      ]
    }
  ]
} 